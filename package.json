{
  "name": "n8n-nodes-sapience",
  "version": "0.1.7",
  "description": "Sapience custom nodes for n8n (agents, utilities, video transcription, fetch, token handling). Connect your sapience account to 1000+ services.",
  "license": "MIT",
  "homepage": "https://github.com/sanchos2913/sapience-n8n",
  "keywords": [
    "n8n-community-node-package",
    "n8n",
    "sapience",
    "automation"
  ],
  "author": {
    "name": "Alex Huk",
    "email": "alexhuk13@gmail.com"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/sanchos2913/sapience-n8n.git"
  },
  "main": "dist/index.js",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "n8n-node build",
    "build:watch": "tsc --watch",
    "dev": "n8n-node dev",
    "lint": "n8n-node lint",
    "lint:fix": "n8n-node lint --fix",
    "release": "n8n-node release",
    "prepublishOnly": "n8n-node prerelease"
  },
  "n8n": {
    "n8nNodesApiVersion": 1,
    "strict": true,
    "credentials": [
      "dist/credentials/GithubIssuesApi.credentials.js",
      "dist/credentials/GithubIssuesOAuth2Api.credentials.js",
      "dist/credentials/SapienceApi.credentials.js"
    ],
    "nodes": [
      "dist/nodes/Sapience/Sapience.node.js"
    ]
  },
  "devDependencies": {
    "@n8n/node-cli": "^0.17.0",
    "@types/node": "^25.0.3",
    "eslint": "^9.32.0",
    "prettier": "^3.6.2",
    "release-it": "^19.0.4",
    "typescript": "^5.9.2"
  },
  "peerDependencies": {
    "n8n-workflow": "*"
  },
  "engines": {
    "node": ">=20.19 <=24.x"
  }
}
